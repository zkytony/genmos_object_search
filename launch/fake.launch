<launch>

  <node name="run_pomdp_agent_fake"
        pkg="sloop_ros" type="run_pomdp_agent" output="screen">
    <param name="~pomdp_name" value="fake"/>
    <!-- So strange. bug you can't do ~config -->
    <rosparam param="config" command="load"
              file="$(find sloop_ros)/config/fake_pomdp.yaml"/>
  </node>

</launch>
